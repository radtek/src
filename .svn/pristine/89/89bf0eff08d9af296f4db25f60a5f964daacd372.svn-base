<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Domain" namespace="cn.justwin.Domain.Entities" xmlns="urn:nhibernate-mapping-2.2">
	<class name="ResUnit" table="Res_Unit" lazy="true" >
		<id name="UnitId">
			<column name="UnitId" sql-type="nvarchar" length="500" not-null="true" />
		</id>
		<property name="Code">
			<column name="Code" sql-type="nvarchar" length="500" not-null="true" />
		</property>
		<property name="Name">
			<column name="Name" sql-type="nvarchar" length="1000" not-null="true" />
		</property>
		<property name="InputUser">
			<column name="InputUser" sql-type="nvarchar" length="20" not-null="false" />
		</property>
		<property name="InputDate">
			<column name="InputDate" sql-type="datetime" not-null="false" />
		</property>
	</class>
</hibernate-mapping>
