        //修改评分
        function changeScore(e) {
            var thisScore = e.attr("alt");
            $("img[name='scoreImg']").each(function () {
                if (parseInt($(this).attr("alt")) > parseInt(thisScore)) {
                    $(this).prop("src", "../resource/images/star-off.png");
                } else {
                    $(this).prop("src", "../resource/images/star-on.png");
                }
            });
