    //修改评价
    var canScore = 1;

    function changeScore(e) {
        if (canScore == 1) {
            var thisScore = e.attr("alt");
            $("img[name='scoreImg']").each(function () {
                if (parseInt($(this).attr("alt")) > parseInt(thisScore)) {
                    $(this).prop("src", "../resource/images/star-off.png");
                } else {
                    $(this).prop("src", "../resource/images/star-on.png");
                }
            });
            var scoreType = "";
            if (thisScore == 1 || thisScore == '1') {
               
                scoreType = "一般";
            }
            if (thisScore == 2 || thisScore == '2') {
                
                scoreType = "良好";
            }
            if (thisScore == 3 || thisScore == '3') {
                
                scoreType = "优秀";
            }
            
            $("#scoreSpan").html(scoreType);
            $("#score").val(thisScore);
        }
    }

    //提交评价
    function submitScore() {
        var score = $("#score").val();
        $.showPreloader('提交中');
        $.ajax({
            type: "POST",
            url: "../Ajax/AjaxGetMsg.aspx/setPFById",
            data: "{diaryId: '" + document.getElementById("KeyID").value + "',userID: '" + GetQueryString("userID") + "',score:'" + score + "'}",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            async: true,
            success: function Success(data) {
                if (data.d != null) {
                    //保存成功时
                    var pf = data.d.split(",")[1];
                    if (pf) {
                        //此时有评价
                        $("#startTaskBtn2").hide();
                        canScore = 1;
                        changeScore($("img[alt='" + pf + "']"));
                        canScore = 0;
                        $.hidePreloader();
                        alert("评价成功");
                    }
                }
                else {
                    $.hidePreloader();
                    alert("评价失败");
                }
            }
        });

    }

    //获取cookie
    function getCookieValue(cookieName) {
        var cookieValue = document.cookie;
        var cookieStartAt = cookieValue.indexOf("" + cookieName + "=");
        if (cookieStartAt == -1) {
            cookieStartAt = cookieValue.indexOf(cookieName + "=");
        }
        if (cookieStartAt == -1) {
            cookieValue = null;
        }
        else {
            cookieStartAt = cookieValue.indexOf("=", cookieStartAt) + 1;
            cookieEndAt = cookieValue.indexOf(";", cookieStartAt);
            if (cookieEndAt == -1) {
                cookieEndAt = cookieValue.length;
            }
            cookieValue = unescape(cookieValue.substring(cookieStartAt, cookieEndAt));//解码latin-1
        }
        return cookieValue;
    }
