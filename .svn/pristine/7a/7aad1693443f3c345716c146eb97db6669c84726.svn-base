@charset "utf-8";
/* CSS Document */
body,ul,ol,li,p,h1,h2,h3,h4,h5,h6,form,fieldset,table,td,img,div{margin:0;padding:0;border:0;}
body{background:url(/images/login/imgs/bg1.gif) repeat-x;color:#333;font-size:12px;font-family:"宋体";}
select,input,img,select{vertical-align:middle;}
a{text-decoration:none;}
a:link{color:#888;}
a:visited{color:#888; }
a:hover,a:active,a:focus{color:#000000;}
.container{ margin:0px auto; padding:0px; height:auto; background:url(/images/login/imgs/bg_2.gif) no-repeat top left;_background:url(/images/login/imgs/bg_22.gif) no-repeat top left;}
#divtop{ margin:0 auto; height:511px;}
.cp_1{ height:186px;margin:0 auto; padding:15px 0 0 150px;}
.cp_2{background: height:208px;margin:0 auto; margin-bottom:-10px;}
.cp_3{background: height:117px;margin:0 auto;}
.cp_4{background:height:125px;}
.cp_5{background: height:120px;}
.copy{background:margin:0 auto;}
.left{float:left; width:348px;height:132px; padding:26px 0 0 20px;}
.right{	float:left; width:300px; margin-left:20px }
.box{width:744px;margin:0 auto;}
.box2{width:650px;height:117px;margin:0 auto;}
.logo3{background:url(/images/login/imgs/logokk.gif) no-repeat;height:169px;margin:20px -110px 0 102px;}
.lock{ background:url(/images/login/imgs/lock.jpg) no-repeat; height:92px; margin:-20px 0 0 160px;}
.key{font-weight:normal; background:url(../imgs/yaoshi-2.jpg) no-repeat; padding-left:43px; height:33px; line-height:2;margin:10px 0 10px -20px;}
form{margin:10px 0 0 40px;}
.name{border:1px #8db8dc solid;width:120px;height:16px;margin-bottom:10px; margin-right:20px;}
.name2{ width:120px;margin-right:30px;}
.yhm{width:55px; display:block; float:left; height:18px; line-height:20px; margin-left:-3px;}
.jz{padding:10px 0 10px 41px;_padding-left:37px;}
.jz input{margin-left:10px; }
.dl{padding-left:44px;}
.dl input{margin-left:7px; margin-top:4px;}
.dl img{margin-left:10px; margin-top:4px;}
span{color:#888;}
.ulright{width:280px;float:right;font-size:12px; list-style:inside;margin-right:5px; margin-top:15px;}
.ulright li{line-height:1.7;color:#888;}
.copyright{margin:0 auto; width:400px;color:#888;display:block; height:100px;}

.lfx{background:url(/images/login/imgs/lfx.jpg) no-repeat; height:27px; margin:-20px 0 0 180px;}
.szc{ font-size:12px;font-weight:normal; color:#666; padding:20px 0 0 0;_padding:40px 0 0 0; }
.ulleft{float:left; width:300px;list-style:inside; color:#888;}
.ulleft li { line-height:2;}
.ulleft li .register {background:url(/images/login/imgs/register.jpg) no-repeat; width:65px; height:19px;text-indent:-9999px;}
.color{color:#003366; font-weight:bold}
.suo{width:24px; height:24px; float:right;margin:17px 40px 0 -15px;_margin-top:-16px; _margin-right:35px;}
.bottom{margin-top:10px;_margin-top:30px;}
.footer{background-color:#0b2640; text-align:center; position:fixed; bottom:0; left:0; width:100%; height:20px; line-height:20px;
        /* for ie6 */ _position:absolute;_bottom:0;overflow:visible; }