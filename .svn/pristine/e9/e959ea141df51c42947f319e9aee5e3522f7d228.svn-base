input[type="checkbox"]{appearance: none; -webkit-appearance: none;outline: none;display:none}
label{width:30px;height:30px;display:inline-block;cursor:pointer;}
label input[type="checkbox"] + span{margin-top:10px;width:20px;height:20px;display:inline-block;background:url(../images/checkbox_01.gif)  no-repeat;background-position:0 0;}
label input[type="checkbox"]:checked + span{margin-top:10px;background-position:0 -21px}
input[type="radio"]{appearance: none; -webkit-appearance: none;outline: none;display:none}
label{width:30px;height:30px;display:inline-block;cursor:pointer;}
label input[type="radio"] + span{margin-top:10px;width:20px;height:20px;display:inline-block;background:url(../images/inputradio.gif)  no-repeat;background-position:0 0;}
label input[type="radio"]:checked + span{margin-top:10px;background-position:0 -21px}
