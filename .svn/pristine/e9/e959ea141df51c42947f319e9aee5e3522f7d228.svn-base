/*前往后台提示框*/
.weui-dialog-mask {
    position: fixed;
    z-index: 1300;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.6);
}

.weui-dialog {
    position: fixed;
    z-index: 1301;
    padding: 30px 20px;
    width: 270px;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background: rgba(249, 249, 249, .96);
    border-radius: 10px;
    box-sizing: border-box;
}

.weui-dialog-hd {
    font-size: 17px;
    font-weight: 500;
    color: #000;
    text-align: center;
    line-height: 24px;
}

.weui-dialog-bd {
    font-size: 14px;
    color: #9b9b9b;
    line-height: 24px;
}

.weui-dialog-bd p {
    margin: 15px 0;
}

.weui-dialog-line {
    position: relative;
    height: 1px;
}

.weui-dialog-line:after {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    border-bottom: 1px solid #e6e6e6;
    color: #e6e6e6;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
    z-index: 2;
}

.weui-dialog-ft {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}

.weui-dialog-btn {
    position: relative;
    display: block;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    text-align: center;
    font-size: 18px;
    line-height: 46px;
    text-decoration: none;
    border-radius: 5px;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

.weui-dialog-btn_primary {
    color: #fff;
    background-color: #2F7DCD;
}

.weui-dialog-btn_default {
    color: #333;
    background-color: #fff;
    border: 1px #ccc solid;
}
