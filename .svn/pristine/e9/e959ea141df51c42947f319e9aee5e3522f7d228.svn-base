/*二级菜单*/
@-webkit-keyframes myanima{
    0%{
        opacity:0;
    }
    50%{
        opacity:0;
    }
    100%{
        opacity:1;
    }
}
.twofloorul {
    position: absolute;
    left: 50%;
    transform: translate(-50%,0);
    width:fit-content;
    width:-moz-fit-content;
    width:-webkit-fit-content;
    margin:auto;
    bottom: 62px;
    text-align: center;
    display: none;
    padding: 0 6px;
    border-radius: 5px;
    border:1px solid #C0BFC4;
    /*box-shadow: 2px -1px 11px 1px rgba(204, 204, 204, 0.7), -2px -1px 11px 0 rgba(204, 204, 204, 0.7);*/
    /*filter: drop-shadow(0 0 11px rgba(204, 204, 204, 0.7));*/
    transition: all 0.15s ease-in-out 0s;
    outline: medium none;
    /*overflow: hidden;*/
    background: #FAFAFA;
    z-index: 1111;
    /*-webkit-animation:myanima 0.15s ;*/
}
.twofloorul:before{
    content: "";
    display: table;
    font: 0/0 a;
    position:absolute;
    bottom: -11px;
    left: 50%;
    margin-left: -3px;
    width: 0;
    height: 0;
    border-width: 5px;
    border-style: solid;
    border-color: #c6c6c6 transparent transparent transparent;
}
.twofloorul:after{
    content: "";
    display: table;
    font: 0/0 a;
    position:absolute;
    bottom: -10px;
    left: 50%;
    margin-left: -3px;
    width: 0;
    height: 0;
    border-width: 5px;
    border-style: solid;
    border-color: #fafafa transparent transparent transparent;
}
ul.twofloorul li {
    min-width: 74px;
    background: none repeat scroll 0 0 #FAFAFA;
    /*border-bottom: 1px solid #c6c6c6;*/
    list-style: none outside none;
    height: 44px;
}
.twofloorul li a:after{
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 1px;
    border-top: 1px solid #C0BFC4;
    color: #C0BFC4;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
    z-index: 2;}
.twofloorul li:first-child a:after{border-top:none !important;}
ul.twofloorul li:last-child{border-bottom:none !important;}
