/*日志统计设置时间选择样式*/
.riliWrap {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    font-size: 18px;
    background-color: #fff;
    z-index: 102;
    display: none;
}
.riliWrap .now {
    height: 50px;
    line-height: 50px;
    position: relative;
    padding: 0 15px;
    display: none;
}
.riliWrap .now::before {
    content: " ";
    position: absolute;
    left: 15px;
    right: 15px;
    top: 0;
    height: 1px;
    border-top: 1px solid #D9D9D9;
    color: #D9D9D9;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}
.riliWrap .cz {
    height: 55px;
    line-height: 55px;
    overflow: hidden;
}
.riliWrap .cz .cancel {
    float: left;
    width: 65px;
    padding-left: 15px;
    cursor: pointer;
}
.riliWrap .cz .enter {
    float: right;
    width: 65px;
    color: #32cd32;
    text-align: right;
    padding-right: 15px;
    cursor: pointer;
}
.riliWrap .riliCon {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    height: 150px;
    overflow: hidden;
    position: relative;
    cursor: pointer;
}
.riliWrap .col,
.riliWrap .colBox {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    text-align: center;
    -webkit-transition: -webkit-transform 0.079s ease-out;
    transition: -webkit-transform 0.079s ease-out;
    transition: transform 0.079s ease-out;
    transition: transform 0.079s ease-out, -webkit-transform 0.079s ease-out;
}
.riliWrap .colBox {
    width: 33.333%;
}
.riliWrap .colBg {
    background: #eee;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 50px;
    z-index: -1;
}
.riliWrap .day {
    height: 50px;
    line-height: 50px;
}
.riliWrap .day.active {
    color: #32cd32;
}
