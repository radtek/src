<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Domain" namespace="cn.justwin.Domain.Entities" xmlns="urn:nhibernate-mapping-2.2">
	<class name="EquEquipmentType" table="Equ_EquipmentType" lazy="true" >
		<id name="Id">
			<column name="Id" sql-type="nvarchar" length="64" not-null="true" />
		</id>
		<property name="ParentId">
			<column name="ParentId" sql-type="nvarchar" length="64" not-null="false" />
		</property>
		<property name="Name">
			<column name="Name" sql-type="nvarchar" length="300" not-null="true" />
		</property>
		<property name="No">
			<column name="No" sql-type="int" not-null="true" />
		</property>
		<property name="Code">
			<column name="Code" sql-type="nvarchar" length="200" not-null="true" />
		</property>
		<property name="Flag">
			<column name="Flag" sql-type="nvarchar" length="50" not-null="false" />
		</property>
	</class>
</hibernate-mapping>