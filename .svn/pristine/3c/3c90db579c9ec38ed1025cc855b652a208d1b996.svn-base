/*----------------------------gridview--------------------------------------------------------------------------------*/
.gvdata
{
	width: 100%; /*table-layout: fixed;*/
	border-collapse: separate;
	border-spacing: 0px 0px;
	border-width: 0px;
	border: solid 1px #CADEED;
}
table.gvdata th
{
        color: #000000;
    font-weight: bold;
    padding-left: 6px;
    padding-right: 6px;
    white-space: nowrap;
    overflow: hidden;
    border: solid 1px #DFDFDF;
}
table.gvdata td
{
    padding-left: 6px;
	padding-right: 6px;  
	border: solid 1px #cccccc;	
    border-left: 0px solid #cccccc;
    border-right: 0px solid #cccccc;
	white-space: nowrap;
}
table.gvdata tr
{
	height: 40px;
}

