<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Domain" namespace="cn.justwin.Domain.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="BasicConfig" table="Basic_Config" lazy="true" >
    <id name="Id">
      <column name="Id" sql-type="nvarchar" length="64" not-null="true" />
    </id>
    <property name="ParaName">
      <column name="ParaName" sql-type="nvarchar" length="200" not-null="true" />
    </property>
    <property name="ParaValue">
      <column name="ParaValue" sql-type="nvarchar" length="200" not-null="false" />
    </property>
    <property name="Note">
      <column name="Note" sql-type="nvarchar(max)" not-null="false" />
    </property>
  </class>
</hibernate-mapping>
