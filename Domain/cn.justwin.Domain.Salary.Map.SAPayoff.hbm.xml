<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Domain" namespace="cn.justwin.Domain.Entities" xmlns="urn:nhibernate-mapping-2.2">
	<class name="SAPayoff" table="SA_Payoff" lazy="true" >
		<id name="Id" column="Id"></id>
		<property name="UserCode">
			<column name="UserCode" sql-type="varchar" not-null="true" />
		</property>
		<property name="Year">
			<column name="Year" sql-type="int" not-null="true" />
		</property>
		<property name="Month">
			<column name="Month" sql-type="int" not-null="true" />
		</property>
		<property name="IsPayoff">
			<column name="IsPayoff" sql-type="bit" not-null="true" />
		</property>
		<property name="BooksId">
			<column name="BooksId" sql-type="nvarchar" length="200" not-null="false" />
		</property>
	</class>
</hibernate-mapping>